!!!strict
%html{html_attrs('en-us')}
  %head
    %title USASearch Analytics
    %meta{'http-equiv' => 'Content-Type', :content => 'text/html; charset=utf-8'}/
    = stylesheet_link_tag 'compiled/analytics.css', :media => 'screen, projection'
    = stylesheet_link_tag 'compiled/print.css', :media => 'print'
    /[if IE]
      = stylesheet_link_tag 'compiled/ie.css', :media => 'screen, projection'
    = javascript_include_tag :defaults
    = setup_google_visualizations
    = calendar_date_select_includes

  %body
    .container
      .span-13
        = link_to(image_tag('USAsearch_medium.gif', :alt=>"USAsearch Home", :width => 201, :height => 44), analytics_home_page_path)
        %h1
          %span.alt= link_to("Analytics",analytics_home_page_path)
          %span.beta Beta
      .span-11.last.header
        = link_to "Query Groups Admin", admin_query_groups_path
        |
        = link_to "FAQ", analytics_faq_path
        |
        = link_to("Home",analytics_home_page_path)
      %hr
      = yield
