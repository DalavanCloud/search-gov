- if search.agency.present? and index == 0 and search.page == 0 and agency_url_matches_by_locale(result['unescapedUrl'], search.agency, I18n.locale)
  = render 'shared/agency_gov_box', :result => result, :search => search, :affiliate => affiliate, :index => index
- else
  .searchresult.clearfix{:id => "searchresult#{index+1}"}
    %h2
      = display_result_extname_prefix(result)
      = display_result_title(result, search, affiliate, index)
    %h3= display_result_description(result)
    %h3= display_result_links(result, search, affiliate, index)
    - if affiliate == nil && index == 0 && search.page == 0
      = display_deep_links_for(result)
