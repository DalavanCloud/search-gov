= affiliate_center_breadcrumbs([link_to(@affiliate.display_name, affiliate_path(@affiliate)), "Add a new RSS Feed"])
%h1 Add a new RSS Feed
= render_help_link(@help_link)
.add-rss-feed
  = form_for([@affiliate, @rss_feed], :html => { :multipart => true }) do |f|
    = render :partial => 'form', :locals => { :form => f }
    = f.submit "Add", :class => 'submit'
    = link_to 'Cancel', affiliate_rss_feeds_path(@affiliate), :class => 'cancel'