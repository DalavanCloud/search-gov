= affiliate_center_breadcrumbs([link_to(@affiliate.display_name, affiliate_path(@affiliate)), "Add a new Boosted Content"])
%h1 Add a new Boosted Content
.add-boosted-content
  = form_for([@affiliate, @boosted_content]) do |f|
    = render :partial => f
    = f.submit "Add"
    = link_to "Cancel", affiliate_boosted_contents_path(@affiliate), :class => 'cancel'
