.uncrawled-urls
  = affiliate_center_breadcrumbs([link_to(@affiliate.display_name, affiliate_path(@affiliate)), link_to('URLs', affiliate_on_demand_urls_path(@affiliate)), "Uncrawled URLs"])
  %h1 Uncrawled URLs
  .uncrawled-url-list
    %table
      %colgroup
        %col{ :class => 'url' }
        %col{ :class => 'actions' }
      %thead
        %tr
          %th{ :colspan => 2 }
            - unless @uncrawled_urls.blank?
              %span= page_entries_info @uncrawled_urls, :entry_name => 'Uncrawled URL', :html => false
      = render :partial => 'uncrawled_urls', :locals => { :shorten_url_length => 30 }
    = will_paginate @uncrawled_urls